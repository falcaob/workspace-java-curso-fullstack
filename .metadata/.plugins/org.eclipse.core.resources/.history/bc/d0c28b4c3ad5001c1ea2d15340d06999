package curso;

import java.util.ArrayList;

public class Main {

	public static void main(String[] args) {
		
		// crear un Array de objeto que tengo que guardar
		// <> que tipo de objeto va a guardar
		// estudiantes nombre array
		ArrayList<Estudiante> estudiantes = new ArrayList<Estudiante>();
	
		// creamos objeto de la clase Estudiantes
		Estudiante estudiante1 = new Estudiante(5, 55, 55555, 0, 0.0, 0.0);
		
		// insertamos el objeto de la clase Estudiante
		estudiantes.add(estudiante1);
				
		
		
		
		
		
		
		// invocacion objeto no estatico OBJETO.METODO_NO_ESTATICO
		estudiante1.mostrarInfo();
				
		// invocacion a metodo estatico: CLASE.METODO_ESTATICO
		Estudiante.crearEstudiante();

	}

}
