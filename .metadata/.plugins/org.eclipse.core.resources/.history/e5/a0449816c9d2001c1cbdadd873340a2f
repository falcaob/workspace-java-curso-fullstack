
public class Estudiante {
	
	// atributos
	int id;
	int edad;
	int telefono;
	int numeroDeNotas;
	double sumaDeNotas;
	double notaMedia;
	
	// constructor por defecto
	public Estudiante () {}
	
	// cosntructor parametrizado
	public Estudiante (int id, int edad, int telefono, int numeroDeNotas, double sumaDeNotas, double notaMedia) {
		this.id = id;
		this.edad = edad;
		this.telefono = telefono;
		this.numeroDeNotas = numeroDeNotas;
		this.sumaDeNotas = sumaDeNotas;
		this.notaMedia = notaMedia;
	}
	
	// metodos
	void mostrarInfo () {}
	void agregarNuevaNota () {}
	

}
